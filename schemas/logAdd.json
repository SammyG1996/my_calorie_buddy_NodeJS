{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "serving_size": {
        "type": "string",
        "minLength": 1,
        "maxLength": 25
      },
      "calories": {
        "type": "number",
        "minLength": 1,
        "maxLength": 30
      },
      "protein": {
        "type": "number",
        "minLength": 1,
        "maxLength": 30
      },
      "carbohydrates": {
        "type": "number",
        "minLength": 1,
        "maxLength": 30
      },
      "fats": {
        "type": "number",
        "minLength": 1,
        "maxLength": 30
      },
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "date": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      }
    },
    "additionalProperties": false,
    "required": [
        "name",
        "serving_size", 
        "calories", 
        "protein", 
        "carbohydrates", 
        "fats", 
        "username", 
        "date"
    ]
  }
  